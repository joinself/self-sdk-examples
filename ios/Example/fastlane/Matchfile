git_url("git@github.com:joinself/ios-example-app-certificates.git")
git_branch("main")

storage_mode("git")

type("appstore") # The default type, can be: appstore, adhoc, enterprise or development

app_identifier(["com.joinself.app.demo"])
username("ops@joinself.com") # Your Apple Developer Portal username

